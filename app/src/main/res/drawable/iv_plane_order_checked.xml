<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/transparent" />
            <stroke
                android:width="1dp"
                android:color="#cccccc" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/plane_mainColor" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>

</layer-list>